<div ng-app="StudentsApp" ng-controller="StudentsController">
    {{status}}

    <form class="form-horizontal" role="form">
        <div class="form-group">
            <label for="inputName3" class="col-sm-2 control-label">Name</label>
            <div class="col-sm-4">
                <input class="form-control" ng-model="formStudentCreate.FirstName">
            </div>
        </div>
        <div class="form-group">
            <label for="inputName3" class="col-sm-2 control-label">Name</label>
            <div class="col-sm-4">
                <input class="form-control" ng-model="formStudentCreate.LastName">
            </div>
        </div>
        <div class="form-group">
            <label for="inputName3" class="col-sm-2 control-label">Name</label>
            <div class="col-sm-4">
                <input class="form-control" ng-model="formStudentCreate.IndexNo">
            </div>
        </div>
        <div class="form-group">
            <label for="inputName3" class="col-sm-2 control-label">Name</label>
            <div class="col-sm-4">
                <input class="form-control" ng-model="formStudentCreate.BirthDate">
            </div>
        </div>
        <div class="form-group">
            <label for="inputName3" class="col-sm-2 control-label">Name</label>
            <div class="col-sm-4">
                <input class="form-control" ng-model="formStudentCreate.BirthPlace">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" class="btn btn-success" ng-click="createStudent()" >Sign up</button>
            </div>
        </div>
        <span>{{formStudentCreate}}</span>
    </form>
</div>
